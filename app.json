{
  "expo": {
    "name": "randomify",
    "scheme": "randomify",
    "slug": "randomify",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "cz.petrkratky.randomify"
    },
    "description": "A React-Native application which generates random albums from Spotify and allows the user to listen to them via the Spotify mobile app."
  }
}